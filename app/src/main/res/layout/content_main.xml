<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/content_main"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.yoshievinsmoke.ayomangan.MainActivity"
    tools:showIn="@layout/app_bar_main">

    <FrameLayout
        android:id="@+id/framekary"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"/>

    <com.example.yoshievinsmoke.ayomangan.tab.SlidingTabLayout
        android:id="@+id/st1_tabs"
        android:layout_below="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.example.yoshievinsmoke.ayomangan.tab.SlidingTabLayout>
    <android.support.v4.view.ViewPager
        android:id="@+id/vp_tabs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/st1_tabs">
    </android.support.v4.view.ViewPager>


</android.support.design.widget.CoordinatorLayout>
